{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["data"],
    "additionalProperties": false,
    "properties": {
        "data": {
            "type": "array",
            "items": {
                "type": "object",
                "required": ["type", "attributes"],
                "properties": {
                    "type": {
                        "const": "answers"
                    },
                    "attributes": {
                        "type": "object",
                        "patternProperties": {
                            "^q-": {
                                "type": ["string", "number", "object", "array", "boolean"]
                            }
                        }
                    }
                }
            }
        },
        "included": {
            "type": "array"
        },
        "meta": {
            "type": "object"
        }
    }
}
