{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": ["data"],
    "properties": {
        "data": {
            "type": "object",
            "additionalProperties": false,
            "required": ["type", "attributes"],
            "properties": {
                "type": {
                    "const": "submissions"
                },
                "attributes": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "status": {
                            "enum": ["NOT_STARTED", "IN_PROGRESS", "COMPLETED", "FAILED"]
                        }
                    }
                }
            }
        }
    }
}
