{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": ["data"],
    "properties": {
        "data": {
            "type": "object",
            "additionalProperties": false,
            "required": ["type", "attributes"],
            "properties": {
                "type": {
                    "const": "questionnaires"
                },
                "attributes": {
                    "type": "object",
                    "additionalProperties": false,
                    "properties": {
                        "templateName": {
                            "type": "string",
                            "pattern": "^[a-zA-Z0-9-]{1,30}$"
                        },
                        "metadata": {
                            "type": "object",
                            "propertyNames": {
                                "pattern": "(^[a-z]|[A-Z0-9])[a-z]*"
                            },
                            "maxProperties": 10,
                            "properties": {
                                "origin": {
                                    "type": "string"
                                },
                                "questionnaireDocumentVersion": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
