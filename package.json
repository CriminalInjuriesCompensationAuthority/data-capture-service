{
    "name": "data-capture-service",
    "version": "5.0.1",
    "scripts": {
        "start": "node ./bin/www",
        "start:dev": "nodemon -L --inspect=0.0.0.0:9229 -e .js,.json,.njk,.yml --ignore openapi/openapi.json --exec npm run build:run",
        "openapi:build": "speccy lint openapi/src/openapi-src.json -j && speccy resolve ./openapi/src/openapi-src.json -j | yaml2json --pretty --indentation 4 --save - > ./openapi/openapi.json && node ./openapi/src/dereference-openapi.js",
        "build:run": "npm run openapi:build && npm run start",
        "pretestx": "eslint .",
        "test": "jest",
        "coverage": "jest --coverage",
        "dump-eslint-conf": "eslint --print-config ./app.js > ./stuff/eslint-conf.json",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "@netflix/nerror": "^1.1.0",
        "@slack/webhook": "^6.0.0",
        "ajv": "^6.12.6",
        "ajv-errors": "^1.0.1",
        "ajv-formats-mobile-uk": "github:CriminalInjuriesCompensationAuthority/ajv-formats-mobile-uk#v1.0.0",
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.9",
        "express": "~4.16.0",
        "express-jwt": "^7.4.3",
        "express-jwt-authz": "^2.4.1",
        "express-openapi-validator": "^4.13.7",
        "got": "^9.6.0",
        "helmet": "^3.21.2",
        "json-translator": "github:CriminalInjuriesCompensationAuthority/json-translator",
        "json-translator-sync": "github:CriminalInjuriesCompensationAuthority/json-translator#sync",
        "jsonpointer": "^5.0.0",
        "lodash.merge": "^4.6.2",
        "moment-timezone": "^0.5.33",
        "morgan": "^1.9.1",
        "notifications-node-client": "^5.1.0",
        "pg": "^7.11.0",
        "pino-http": "^5.5.0",
        "q-router": "github:CriminalInjuriesCompensationAuthority/q-router#v2.6.0",
        "q-templates-application": "github:CriminalInjuriesCompensationAuthority/q-templates-application#node16",
        "swagger-ui-express": "^4.3.0",
        "uuid": "^3.3.2",
        "verror": "^1.10.0"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "fs-extra": "^8.1.0",
        "husky": "^4.0.3",
        "jest": "^26.6.3",
        "jest-json-schema": "^2.0.2",
        "json-schema-ref-parser": "^6.1.0",
        "lint-staged": "^9.5.0",
        "nanoid": "^2.0.1",
        "nodemon": "^1.18.9",
        "pino-pretty": "^3.2.0",
        "prettier": "^1.19.1",
        "speccy": "github:wework/speccy#740d19d88935db7735250c16abc2ad09256b5854",
        "supertest": "^4.0.2",
        "uuidv4": "^4.0.0",
        "yamljs": "^0.3.0"
    },
    "license": "MIT"
}
