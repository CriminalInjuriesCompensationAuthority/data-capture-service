{
    "name": "data-capture-service",
    "version": "1.6.0",
    "scripts": {
        "start": "node ./bin/www",
        "start:dev": "nodemon -L --inspect=0.0.0.0:9229 -e .js,.json,.njk,.yml --ignore openapi/openapi.json --exec npm run build:run",
        "openapi:build": "speccy lint openapi/src/openapi-src.json -j && speccy resolve ./openapi/src/openapi-src.json -j | yaml2json --pretty --indentation 4 --save - > ./openapi/openapi.json && node ./openapi/src/dereference-openapi.js",
        "build:run": "npm run openapi:build && npm run start",
        "pretestx": "eslint .",
        "test": "jest",
        "coverage": "jest --coverage",
        "dump-eslint-conf": "eslint --print-config ./app.js > ./stuff/eslint-conf.json",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "@slack/webhook": "^6.0.0",
        "ajv": "^6.10.0",
        "ajv-errors": "^1.0.1",
        "ajv-formats-mobile-uk": "github:CriminalInjuriesCompensationAuthority/ajv-formats-mobile-uk#v1.0.0",
        "cookie-parser": "~1.4.3",
        "express": "~4.16.0",
        "express-jwt": "^5.3.1",
        "express-jwt-authz": "^2.3.1",
        "express-openapi-validator": "^1.6.2",
        "got": "^9.6.0",
        "helmet": "^3.21.2",
        "json-pointer": "^0.6.0",
        "lodash.merge": "^4.6.2",
        "moment-timezone": "^0.5.33",
        "notifications-node-client": "^4.7.0",
        "pg": "^7.11.0",
        "pino-http": "^5.5.0",
        "pino-pretty": "^4.7.1",
        "q-router": "github:CriminalInjuriesCompensationAuthority/q-router#v2.5.3",
        "swagger-ui-express": "^4.0.2",
        "uuid": "^3.3.2",
        "verror": "^1.10.0"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^24.9.0",
        "jest-json-schema": "^2.0.2",
        "json-schema-ref-parser": "^6.1.0",
        "nodemon": "^1.18.9",
        "prettier": "^1.19.1",
        "speccy": "github:wework/speccy#740d19d88935db7735250c16abc2ad09256b5854",
        "supertest": "^4.0.2",
        "yamljs": "^0.3.0"
    },
    "license": "MIT",
    "_m0": {
        "devDependencies": {
            "eslint": "^6.8.0",
            "eslint-config-airbnb-base": "^14.0.0",
            "eslint-config-prettier": "^6.9.0",
            "eslint-plugin-import": "^2.19.1",
            "eslint-plugin-jest": "^23.3.0",
            "eslint-plugin-prettier": "^3.1.2",
            "fs-extra": "^8.1.0",
            "husky": "^4.0.3",
            "jest": "^24.9.0",
            "lint-staged": "^9.5.0",
            "prettier": "^1.19.1"
        },
        "blocks": [
            ".eslintignore",
            ".eslintrc.js",
            ".huskyrc.js",
            ".lintstagedrc.js",
            ".prettierrc.js",
            ".gitignore",
            "jest.config.js"
        ],
        "files": [
            ".editorconfig",
